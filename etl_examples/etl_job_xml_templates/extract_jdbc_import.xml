<?xml version="1.0"?>
<?xml-stylesheet type="text/css" href="xmlpartsstyle.css"?>
<etl_job xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../etl_job_conf/etl_feed_job.xsd">
    <job_static_param>
        <feed_name>feed-name</feed_name>
        <frequency>DAILY</frequency>
    </job_static_param>

    <extract>
        <type>JDBC</type>
        <format_file_name>true</format_file_name>
        <db_property_file_path>{db-property-file-path}</db_property_file_path>
        <sql_query_file_path>{sql-query-file-path.sql}</sql_query_file_path>
        <query_params>
            <param order="1" name="{col1}" value="FIRST_DATE"/>
        </query_params>
    </extract>

    <transform>
        <rule order="1" type="SCHEMA_TRANSFORMATION" group="1" failed_field_limit="0" failed_row_limit="10"/>
    </transform>

    <load>
        <type>HIVE</type>
        <task>{load-task-name}</task>
        <db_name>{db-name}</db_name>
        <table_name>{table-name}</table_name>
        <file_type>PARQUET</file_type>
        <partition_data>
            <coalesce_partition>true</coalesce_partition>
            <overwrite_partition>true</overwrite_partition>
            <coalesce_partition_count>10</coalesce_partition_count>
            <partition_file_initial_path>{partition-file-path-initial}</partition_file_initial_path>
            <partition_columns>
                <column name="{partition-col1}" value="DATE"/>
            </partition_columns>
        </partition_data>
    </load>
</etl_job>